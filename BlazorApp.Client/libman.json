{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "wwwroot/css/bootstrap/"
    },
    {
      "library": "bootstrap-icons@1.8.1",
      "destination": "wwwroot/css/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "datatables.net@2.1.1",
      "destination": "wwwroot/lib/datatables.net/"
    },
    {
      "library": "datatables.net-bs5@1.11.5",
      "destination": "wwwroot/lib/datatables.net-bs5/"
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "select2@4.1.0-rc.0",
      "destination": "wwwroot/lib/select2/"
    }
  ,
{
  "provider": "cdnjs",
  "library": "select2-bootstrap-5-theme@1.2.0",
  "destination": "wwwroot/lib/select2-bootstrap-5-theme/"
}
]
}